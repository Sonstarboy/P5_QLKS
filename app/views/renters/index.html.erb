<p id="notice"><%= notice %></p>

<h1>Renters</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Sex</th>
      <th>Address</th>
      <th>Id number</th>
      <th>Phone</th>
      <th>Citizenship</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @renters.each do |renter| %>
      <tr>
        <td><%= renter.name %></td>
        <td><%= renter.sex %></td>
        <td><%= renter.address %></td>
        <td><%= renter.id_number %></td>
        <td><%= renter.phone %></td>
        <td><%= renter.citizenship %></td>
        <td><%= link_to 'Show', renter %></td>
        <td><%= link_to 'Edit', edit_renter_path(renter) %></td>
        <td><%= link_to 'Destroy', renter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Renter', new_renter_path %>
