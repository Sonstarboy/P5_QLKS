<%= simple_form_for(@renter) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :name %>

    <%= f.input :sex %>

    <%= f.input :address %>

    <%= f.input :id_number %>

    <%= f.input :phone %>

    <%= f.input :citizenship %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
