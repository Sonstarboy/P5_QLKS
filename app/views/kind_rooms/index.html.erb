<p id="notice"><%= notice %></p>

<h1>Kind Rooms</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kind_rooms.each do |kind_room| %>
      <tr>
        <td><%= kind_room.name %></td>
        <td><%= kind_room.code %></td>
        <td><%= link_to 'Show', kind_room %></td>
        <td><%= link_to 'Edit', edit_kind_room_path(kind_room) %></td>
        <td><%= link_to 'Destroy', kind_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kind Room', new_kind_room_path %>
