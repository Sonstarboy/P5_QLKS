<%= simple_form_for(@agreement) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :code %>

    <%= f.association :renter %>

    <%= f.association :employee %>

    <%= f.input :date_contract %>

    <%= f.input :status %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
